    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "chai": "^5.1.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.4"
  },
  "peerDependencies": {
    "typescript": "^5.3.2"
